{
    "name": "mockmark",
    "version": "0.1.0",
    "description": "Development-only visual indicators for mock data in React applications",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "devDependencies": {
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "keywords": [
        "react",
        "mock",
        "development",
        "debugging",
        "visualization",
        "indicator",
        "mock-data"
    ],
    "author": "jlee",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/jlee/mockmark"
    },
    "sideEffects": false
}