{
    "name": "@mockmark/angular",
    "version": "0.2.0",
    "description": "Development-only visual indicators for mock data in Angular applications",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup"
    },
    "peerDependencies": {
        "@angular/common": ">=17.0.0",
        "@angular/core": ">=17.0.0"
    },
    "dependencies": {
        "@mockmark/core": "workspace:*",
        "tslib": "^2.6.0"
    },
    "devDependencies": {
        "@angular/common": "^19.0.0",
        "@angular/core": "^19.0.0",
        "rxjs": "^7.8.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0",
        "zone.js": "^0.15.0"
    },
    "keywords": [
        "angular",
        "mock",
        "development",
        "debugging",
        "visualization",
        "indicator",
        "mock-data"
    ],
    "author": "jlee",
    "license": "MIT",
    "sideEffects": false
}