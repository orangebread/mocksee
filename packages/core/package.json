{
    "name": "@mockmark/core",
    "version": "0.2.0",
    "description": "Shared core logic for MockMark - development-only visual indicators",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "keywords": [
        "mockmark",
        "core",
        "mock-data"
    ],
    "author": "jlee",
    "license": "MIT",
    "sideEffects": false
}